<h2>Lista de Speakers </h2>        
<div class="table-responsive">          
    <table class="table">
        <thead>
            <tr>   
                <th>nome</th> 
                <th>telefone</th>     					
                <th>nif</th>
                <th>cachet</th>
                <th>email</th>
                <th>dia_sessao</th>					   
            </tr>
        </thead>
        <tbody>
    <% for (var u of data) { %>       
            <tr>
                <td><a href="/speakers/<%= u.nome %>"><%= u.nome %></a></td>
                <td><%= u.telefone %></td>
                <td><%= u.nif %></td>
                <td><%= u.cachet %></td>
                <td><%= u.email %></td>
                <td><%= u.dia_sessao %></td>
            </tr>
    <%} %>      
        </tbody>
    </table>
    <button type="button" class="btn btn-default" onclick="window.open('/speakers/create')">Novo Speaker</button>
</div>       
